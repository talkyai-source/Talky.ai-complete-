# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Environment
ENVIRONMENT=development

# STT Providers
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# TTS Providers
CARTESIA_API_KEY=your_cartesia_api_key_here

# LLM Providers
GROQ_API_KEY=your_groq_api_key_here

# Telephony
VONAGE_API_KEY=your_vonage_api_key_here
VONAGE_API_SECRET=your_vonage_api_secret_here
VONAGE_APP_ID=your_vonage_app_id_here

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/voice_dialer
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_KEY=your_supabase_service_role_key_here

# JWT Verification (Required for production - get from Supabase Dashboard → Settings → API → JWT Secret)
SUPABASE_JWT_SECRET=your_jwt_secret_here

# Redis
REDIS_URL=redis://localhost:6379

# Application
SECRET_KEY=your_secret_key_here_change_in_production
